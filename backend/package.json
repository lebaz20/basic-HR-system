{
  "name": "basic-hr-system-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-backend": "export NODE_ENV=development; webpack --mode development --watch",
    "start-frontend": "cd ../frontend; yarn start",
    "start": "yarn start-backend & yarn start-frontend",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "lint-backend": "eslint src sequelize",
    "lint-frontend": "cd ../frontend; yarn lint",
    "lint": "yarn lint-backend && yarn lint-frontend"
  },
  "dependencies": {
    "@babel/register": "^7.6.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.19.8"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon-webpack-plugin": "^4.1.1",
    "prettier": "^1.18.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  }
}
